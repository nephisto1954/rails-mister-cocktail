<div class="container">
  <h1>List of Cocktails </h1>

  <% @cocktails.each do |cocktail| %>
  <ul>
    <li>
      <%= link_to cocktail.name, cocktail_path(cocktail) %>
    </li>
  </ul>
  <% end %>

  <a class="btn btn-primary btn-lg"> Create a New Cocktail<%link_to "Create a new Cocktail", new_cocktail_path  %></a>

</div>

